 static void Main()

    {
        string str1 = "hello";
        string str2 = "Hello";
        int result = str1.CompareTo(str2);
        if (result == 0)
        {
            Console.WriteLine("兩個字串相等。");
        }
        else
        {
            Console.WriteLine("兩個字串不同。");
        }
    }
